{
    "name": "Hide Next-up",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Hide Next-up in web video player",
    "minimum_chrome_version": "102",
    "content_scripts": [
        {
            "matches": [
                "https://www.amazon.com/*",
                "https://www.amazon.co.jp/*"
            ],
            "js": [
                "./src/js/amazon.js",
                "./src/js/netflix.js",
                "./src/js/main.js"
            ]
        }
    ]
}